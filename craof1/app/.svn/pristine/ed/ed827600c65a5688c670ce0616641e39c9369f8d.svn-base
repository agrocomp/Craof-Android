<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Etapa 2/5"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:id="@+id/progressBar4"
        android:layout_width="match_parent"
        android:layout_height="20dip"
        android:layout_gravity="center_horizontal"
        android:indeterminate="false"
        android:max="5"
        android:progress="2"
        android:layout_below="@+id/textView35"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Insumo"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="30dip"
        android:layout_below="@+id/progressBar4"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="195dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/etAplicacaoInsumoInsumo"
        android:layout_gravity="top"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        android:id="@+id/btAplicacaoInsumoInsumoVoltar"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/textView35"
        android:layout_toStartOf="@+id/textView35" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PrÃ³ximo"
        android:id="@+id/btAplicacaoInsumoInsumoProximo"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/textView36"
        android:layout_toEndOf="@+id/textView36" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancelar"
        android:id="@+id/btAplicacaoInsumoInsumoCancelar"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/textView36"
        android:layout_alignStart="@+id/textView36" />
</RelativeLayout>