    public void setCultura(Cultura cultura) {
        plantio.setCultura(cultura);
        this.wtCultura.setText(cultura.getNome());
    }

    public void setLocal(LocalPlantio local) {
        plantio.setLocalPlantio(local);
        this.wtLocal.setText(local.getDescricao());
    }


