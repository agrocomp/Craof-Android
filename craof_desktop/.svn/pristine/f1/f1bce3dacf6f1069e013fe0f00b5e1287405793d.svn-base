    private void busarCulturaActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_busarCulturaActionPerformed
        FrameAuxiliar c = new FrameAuxiliar(this);
        c.abrirPanel(new PanelCulturaBuscar(c), "Buscar Cultura - CRAOF");

        //FrameCulturaPesquisa c = new FrameCulturaPesquisa(this);
        c.setVisible(true);

    }//GEN-LAST:event_busarCulturaActionPerformed

    private void salvarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_salvarActionPerformed
        controle = new Controle_Destino_Produtos();
        destinoProduto.setDataColheita(wtData.getText());
        destinoProduto.setQuantidade(Integer.parseInt(wtQuantidade.getText()));
        try {
            if (!controle.salvar(destinoProduto)) {
                JOptionPane.showMessageDialog(null, "Compra de insumo salvo com sucesso!");
            } else {
                JOptionPane.showMessageDialog(null, "ERROR CRAOFx03 : Falha ao salvar compra de insumos!");
            }
        } catch (SQLException ex) {
            Logger.getLogger(DestinoProdutos.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_salvarActionPerformed

    private void cancelarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cancelarActionPerformed
