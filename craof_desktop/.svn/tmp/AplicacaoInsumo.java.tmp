    public JTextField getWtInsumo() {
        return wtInsumo;
    }

    public void setInsumo(Insumo insumo) {
        this.wtInsumo.setText(insumo.getDescricao());
        aplicacaoInsumo.setInsumo(insumo);
    }
    
    public void setCultura(Cultura cultura){
        aplicacaoInsumo.setCultura(cultura);
        wtCultura.setText(cultura.getNome());
    }
    public void setLocal(LocalPlantio local){
        aplicacaoInsumo.setLocalPlantio(local);
        wtLocal.setText(local.getDescricao());
    }

    

